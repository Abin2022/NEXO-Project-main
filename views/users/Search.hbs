                  success: (response) => {

                    if (response.COD_CHECKOUT) {
                        location.href = '/orderPlaced'
                        
                    }else if(response.WALLET_CHECKOUT){
                        location.href = '/wallet-placed?id='+response.orderId
                    }else if(response.error){
                         $('#error-message').text(response.error);
